{
  "plugins": [
    "transform-runtime",
    "transform-flow-strip-types",
    "transform-class-properties",
     ["transform-class-properties", { "spec": true }]
  ],
  "presets": [
    "stage-0",
    "stage-1",
    "stage-2",
    "stage-3",
    "flow",
    ["env", {
      "targets": {
        "node": "8"
      },
      "useBuiltIns": "entry"
    }]
  ]
}
