{
  "name": "es7-env",
  "version": "1.0.0",
  "private": true,
  "description": "An isolated docker based environment to run the latest ES2017 (ES7) features",
  "scripts": {
    "prestart": "babel-node build/startMessage.js",
    "start": "npm-run-all --parallel security:check open:src lint:watch",
    "open:src": "babel-node build/srcServer.js",
    "lint": "esw webpack.config.* app/src build --color",
    "lint:watch": "npm run lint -- --watch",
    "security:check": "nsp check",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node build/build.js",
    "es7:make": "docker build -t es7-env .",
    "es7": "docker stop es7; docker run --rm -it -p 3000:3000 -p 5858:5858 -p 35729:35729 -v \"$PWD:/var/app\" -v es7-node-modules:/var/app/node_modules --name es7 es7-env bash; exit 0",
    "es7:sh": "docker exec -it es7 bash; exit 0",
    "es7:node": "docker exec -it es7 node ./app/src/${F}; exit 0",
    "es7:clean": "docker stop es7; docker rmi es7-env; exit 0",
    "es7:prune": "npm run es7:clean; node_image=$(docker images node -q); [[ $node_image ]] && docker rmi $node_image; npm_volume=$(docker volume ls -q | grep es7-node-modules); [[ $npm_volume ]] && docker volume rm $npm_volume; exit 0"
  },
  "author": "Itai Koren",
  "license": "MIT",
  "dependencies": {
    "axios": "latest",
    "babel-polyfill": "7.0.0-alpha.15",
    "bootstrap": "4.0.0-alpha.6",
    "vue": "latest",
    "vue-codemirror-lite": "latest",
    "vue-form": "latest",
    "vue2-loading-bar": "latest"
  },
  "devDependencies": {
    "babel-cli": "^7.0.0-alpha.15",
    "babel-core": "^7.0.0-alpha.15",
    "babel-loader": "^7.0.0-alpha.15",
    "babel-plugin-transform-class-properties": "^7.0.0-alpha.17",
    "babel-plugin-transform-runtime": "^7.0.0-alpha.15",
    "babel-preset-env": "^2.0.0-alpha.12",
    "babel-preset-es2017": "latest",
    "babel-preset-flow": "^7.0.0-alpha.15",
    "babel-preset-latest": "^7.0.0-alpha.7",
    "babel-preset-stage-0": "^7.0.0-alpha.15",
    "babel-preset-stage-1": "^7.0.0-alpha.15",
    "babel-preset-stage-2": "^7.0.0-alpha.15",
    "babel-preset-stage-3": "^7.0.0-alpha.15",
    "babel-register": "^7.0.0-alpha.15",
    "body-parser": "latest",
    "chalk": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-watch": "latest",
    "esprima": "latest",
    "express": "latest",
    "extract-text-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    "imports-loader": "latest",
    "npm-run-all": "latest",
    "nsp": "latest",
    "raw-loader": "latest",
    "rimraf": "latest",
    "style-loader": "latest",
    "vue-loader": "latest",
    "vue-template-compiler": "latest",
    "webpack": "latest",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "latest",
    "webpack-livereload-plugin": "latest",
    "webpack-md5-hash": "latest"
  }
}
